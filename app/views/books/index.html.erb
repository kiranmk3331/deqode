<h1>List of Books</h1>
<table>
  <tr>
    <th>Title</th>
    <th>Author</th>
    <th>Published Year</th>
    <!-- Add other attributes here as needed -->
  </tr>
  <% @books.each do |book| %>
    <tr>
      <td><%= book.title %></td>
      <td><%= book.author %></td>
      <td><%= book.published_year %></td>
      <!-- Add other attributes here as needed -->
    </tr>
  <% end %>
</table>
